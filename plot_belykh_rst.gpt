#!/usr/bin/gnuplot

set term postscript eps enhanced color font 'Helvetica,20'
set output "belykh_rst_N_K.eps"
set termoption dashed

set key above
set grid

f1="belykh_{N:100}_{om0:2}_{delta:0.02}_{alpha:PI_6}.dat"
#f2="belykh_{N:100}_{om0:2}_{delta:0.02}_{alpha:PI_6}.dat"

set style line 1 lt 1 pt 4 ps 1 lw 1 lc rgb 'blue'
set style line 2 lt 2 pt 19 ps 1 lw 1 lc rgb 'blue'

plot  f1 u 1:($2/10.0) w lp ls 1 t 'N=10, {/Symbol a}={/Symbol p}/6, Numer',\
      f1 u 1:($3/10.0) w lp ls 2 t 'N=10, {/Symbol a}={/Symbol p}/6, Theor'
