#!/usr/bin/gnuplot

#set term postscript eps enhanced color font 'Helvetica,20'
#set output "K_alpha.eps"
#set termoption dashed

set key above
set grid

f1="belykh_{N=10}_{om0=2}_{delta=1}_alpha_changing.dat"

set style line 1 lt 1 pt 4 ps 2 lw 1 lc rgb 'magenta'
set style line 2 lt 2 pt 19 ps 1 lw 1 lc rgb 'green'
set style line 3 lt 2 pt 10 ps 2  lw 1 lc rgb 'red'
set style line 4 lt 2 pt 7 ps 1 lw 1 lc rgb 'blue'
set style line 5 lt 2 pt 2 ps 1 lw 1 lc rgb 'black'

set xlabel '{/Symbol a}'
set ylabel 'K_{sync}'

plot f1 u 1:2 w lp ls 1 title 'Numeric', \
     f1 u 1:3 w lp ls 2 title 'Upper bound', \
     f1 u 1:(cos($1/2)/sin($1)) w lp lc rgb 'black' title 'Lower bound', \
     f1 u 1:(($3/$2)) w lp ls 3 title 'Numeric error', \
     f1 u 1:(1/(cos($1/2)*cos(3*$1/2))) w lp ls 4 title 'Max error'

